programa buenas
procesos
  proceso hola (ES flores : numero)
  variables
    camello: numero
  comenzar
    flores:= 0
    camello:= 10
    mover
    mientras HayFlorEnLaEsquina
      flores:=flores+1 
      tomarFlor
      camello:= (camello - 1)
      si camello <= 5
        mover
  fin
areas
  ciudad: AreaC (1,1,100,100)
robots
  robot robot1
  variables
    mama: numero
  comenzar
    mover
    hola (mama)
    Informar (mama)
  fin
variables
  R-info: robot1
comenzar
  AsignarArea (R-info, ciudad)
  Iniciar (R-info, 1,1)
fin
